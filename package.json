{
  "name": "telegram-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:pooling": "ts-node ./pooling.ts",
    "start:webhook": "ts-node ./webhook.ts",
    "prisma_migrate":"prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma_migrate_deploy":"prisma migrate deploy --schema=./prisma/schema.prisma",
    "prisma_generate": "prisma generate",
    "prisma_migrate_resolve": "prisma migrate resolve --applied 0_init",
    "prisma_migrate_diff": "npx prisma migrate diff --from-empty --to-schema-datamodel prisma/schema.prisma --script > prisma/migrations/0_init/migration.sql",
    "postprisma_migrate_diff": "npm run prisma_migrate_resolve",
    "postprisma_migrate_resolve": "npm run prisma_migrate_deploy",
    "prestart:webhook": "npm run prisma_migrate_diff"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "@types/express": "^4.17.17",
    "express": "^4.18.2",
    "grammy": "^1.16.1"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "prisma": "^4.15.0",
    "ts-node": "^10.9.1"
  }
}
